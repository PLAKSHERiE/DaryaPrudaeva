        include circle.pug
        .fit__box
            .fit__col
                .fit__title.title Кому подойдет?
                .fit__item
                    .fit__item-icon.fit__item-icon--bottom-left
                        <svg width="50" height="50" viewBox="0 0 50 50" fill="none" xmlns="http://www.w3.org/2000/svg">
